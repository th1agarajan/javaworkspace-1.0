		<action forward="displayTags" path="/displayTags" />
		<action forward="pagerTags" path="/pagerTags" />
		<action forward="paginationInServlets" path="/paginationInServlets" />
		<action forward="paginationUsingJavaScript" path="/paginationUsingJavaScript" />

		<action forward="sitemap" path="/sitemap" />
		<action forward="contactUs" path="/contactUs" />
		<action forward="termsOfUse" path="/termsOfUse" />
		<action forward="privacyPolicy" path="/privacyPolicy" />

	</action-mappings>


	<controller processorClass="org.apache.struts.tiles.TilesRequestProcessor" />
	<message-resources null="false" parameter="MessageResources" />
	<plug-in className="org.apache.struts.tiles.TilesPlugin">
		<set-property property="definitions-config" value="/WEB-INF/tiles-defs.xml" />
		<set-property property="moduleAware" value="true" />
	</plug-in>
	<plug-in className="org.apache.struts.validator.ValidatorPlugIn">
		<set-property property="pathnames"
			value="/WEB-INF/validator-rules.xml,/WEB-INF/validation.xml" />
	</plug-in>
</struts-config>